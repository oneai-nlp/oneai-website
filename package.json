{
  "dependencies": {
    "jquery": "^3.7.1",
    "prettier": "^3.2.5",
    "rollup": "^4.12.0",
    "terser": "^5.29.2"
  },
  "scripts": {
    "rollup": "npx rollup --bundleConfigAsCjs --config",
    "terser": "npx terser out/main.js -o out/main.min.js --config-file ./terser.config.json",
    "build": "npm run rollup && npm run terser",
    "clipboard": "pbcopy < main.min.js"
  }
}
